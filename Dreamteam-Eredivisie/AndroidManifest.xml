        <activity
            android:name="com.example.dreamteam_eredivisie.ShowTeamActivity"
            android:label="@string/title_activity_show_team" >
        </activity>
        <activity
            android:name="com.example.dreamteam_eredivisie.DatabaseFilterActivity"
            android:label="@string/title_activity_database_filter" >
        </activity>
        <activity
            android:name="com.example.dreamteam_eredivisie.FilteredDatabaseActivity"
            android:label="@string/title_activity_filtered_database" >
        </activity>
        <activity
            android:name="com.example.dreamteam_eredivisie.PostTeamActivity"
            android:label="@string/title_activity_post_team" >
